generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model url {
  id         String    @id @default(uuid())
  created_at DateTime  @default(now())
  shorted    String    @unique
  long       String
  qrcode     String
  expired_at DateTime?
}
